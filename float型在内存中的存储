#include <stdio.h>
int main()
{
	int n = 9;
	//0 00000000 00000000000000000001001-9的补码
	float *pFloat = (float*)&n;
	printf("n的值为：%f\n", n);//9
	printf("pFloat的值为：%f\n", pFloat);//0.00000
	//(-1)^0*0.0000000000000000000000001001*2^-126
	//
	*pFloat = 9.0;
	//1001.0
	//(-1)^0*1.001&2^3
	//01000001000100000000000000000000
	//1091567616--十进制

	printf("num的值为:%d\n", n); //1091567616
	printf("pFloat的值为:%f\n", *pFloat);//9.000000
	return 0;
}
